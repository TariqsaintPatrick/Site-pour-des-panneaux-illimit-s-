  <script>
    document.addEventListener('DOMContentLoaded', () => {
      const elements = document.querySelectorAll('h1, .tagline, .profile, .cta-buttons, .features');
      
      elements.forEach((el, index) => {
        setTimeout(() => {
          el.style.opacity = '1';
          el.style.transform = 'translateY(0)';
        }, index * 200);
        
        el.style.opacity = '0';
        el.style.transform = 'translateY(20px)';
        el.style.transition = 'opacity 0.5s ease, transform 0.5s ease';
      });
      
      const featureCards = document.querySelectorAll('.feature-card');
      featureCards.forEach(card => {
        card.addEventListener('mousemove', (e) => {
          const rect = card.getBoundingClientRect();
          const x = e.clientX - rect.left;
          const y = e.clientY - rect.top;
          
          card.style.setProperty('--mouse-x', `${x}px`);
          card.style.setProperty('--mouse-y', `${y}px`);
        });
      });
      
      const showPanelsBtn = document.getElementById('showPanelsBtn');
      const panelsContainer = document.getElementById('panelsContainer');
      
      showPanelsBtn.addEventListener('click', () => {
        if (panelsContainer.style.display === 'none') {
          panelsContainer.style.display = 'grid';
          setTimeout(() => {
            panelsContainer.style.opacity = '1';
            panelsContainer.style.transform = 'translateY(0)';
          }, 10);
          showPanelsBtn.innerHTML = '<i class="fas fa-eye-slash"></i> Masquer les Offres';
        } else {
          panelsContainer.style.opacity = '0';
          panelsContainer.style.transform = 'translateY(20px)';
          setTimeout(() => {
            panelsContainer.style.display = 'none';
          }, 300);
          showPanelsBtn.innerHTML = '<i class="fas fa-gem"></i> Voir Nos Offres';
        }
      });
      
     
      panelsContainer.style.opacity = '0';
      panelsContainer.style.transform = 'translateY(20px)';
      panelsContainer.style.transition = 'opacity 0.5s ease, transform 0.5s ease';
    });
  </script>
